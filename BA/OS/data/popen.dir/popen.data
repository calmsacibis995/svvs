:POPEN,"R":
# This is a comment
# pop0 does the following:
#	pop0 -x exitval		exits with value exitval
#	pop0 -s signum		kills self with signal signum
#	pop0 -t waitval		sleeps for waitval seconds before execution
#	pop0 [arglist] -o 	writes args to stdout, one per line
#	pop0 [arglist] -o ofile	writes args to ofile, one per line
# 	pop0 -i 		reads stdin
#	pop0 -i ifile		reads from input file ifile
#	pop0 -i -o		copies stdin to stdout
#	pop0 -i -o ofile	copies stdin to ofile
#	pop0 -i ifile -o	copies ifile to stdout
#	pop0 -i ifile -o ofile	copies ifile to ofile
#	pop0 -p procnum		kills procnum with signal SIGKILL
#	pop0 -s sig -p procnum	kills procnum with signal sig
#	pop0 -e			report envp instead of argv
#	
#	any combination of args except -x and -s will work
#	error messages are reported on stderr
#
:EOT:

